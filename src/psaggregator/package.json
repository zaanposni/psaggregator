{
    "name": "psaggregator",
    "version": "1.12.0",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check . && eslint .",
        "prismagenerate": "prisma generate --schema ./src/config/schema.prisma",
        "prismamigrate": "prisma migrate dev --schema ./src/config/schema.prisma",
        "prismastudio": "prisma studio --schema ./src/config/schema.prisma",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@fontsource/fira-mono": "^4.5.10",
        "@neoconfetti/svelte": "^1.0.0",
        "@prisma/client": "4.15.0-integration-feat-client-esm.27",
        "@sveltejs/adapter-auto": "^3.0.0",
        "@sveltejs/adapter-node": "^4.0.1",
        "@sveltejs/kit": "^2.4.3",
        "@sveltejs/vite-plugin-svelte": "^3.0.0",
        "@tailwindcss/forms": "^0.5.7",
        "@types/eslint": "8.56.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "autoprefixer": "^10.4.16",
        "axios": "^1.6.2",
        "carbon-icons-svelte": "^12.4.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.35.1",
        "highlight.js": "^11.9.0",
        "moment": "^2.30.1",
        "postcss": "^8.4.32",
        "postcss-load-config": "^5.0.2",
        "prettier": "^3.1.1",
        "prettier-plugin-svelte": "^3.1.2",
        "prettier-plugin-tailwindcss": "^0.5.9",
        "prisma": "4.15.0-integration-feat-client-esm.27",
        "svelte": "^4.2.7",
        "svelte-check": "^3.6.0",
        "tailwindcss": "^3",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "vite": "^5.0.12"
    },
    "type": "module",
    "dependencies": {
        "@internationalized/date": "^3.5.5",
        "bits-ui": "^0.21.13",
        "clsx": "^2.1.1",
        "lucide-svelte": "^0.441.0",
        "mode-watcher": "^0.4.1",
        "svelte-sonner": "^0.3.28",
        "tailwind-merge": "^2.5.2",
        "tailwind-variants": "^0.2.1"
    }
